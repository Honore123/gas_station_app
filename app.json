{
  "expo": {
    "name": "Nziza",
    "slug": "Nziza",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_nz.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.nzizacrafts.nziza",
      "versionCode": 8
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
